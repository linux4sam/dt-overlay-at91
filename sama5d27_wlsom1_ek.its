			fdt = "fdt_pda7b";
		};

		ptc {
			description = "FDT overlay blob for PTC";
			fdt = "fdt_ptc";
		};

		spi_click1 {
			description = "FDT overlay blob for at25ff321a spi flash clickboard (1)";
			fdt = "fdt_spi_click1";
		};
	};
};
